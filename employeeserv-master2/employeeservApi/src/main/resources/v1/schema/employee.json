{
	"title": "Employee resource",
	"description": "Employee resource object",
	"type": "object",
	"properties": {
		"id": {
			"description": "employee id",
			"type": "integer"
		},
		"first_name": {
			"description": "first name",
			"type": "string",
			"minLength": 1,
			"maxLength": 255
		},
		"last_name": {
			"description": "last name",
			"type": "string",
			"minLength": 1,
			"maxLength": 255
		},
		"date_of_birth": {
			"description": "date_of_birth name",
			"type": "date",
			"format": "date-time"
		},
		"address": {
			"type": "object",
			"properties": {
				"line1": {
					"type": "string"
				},
				"line2": {
					"type": "string",
					"default": ""
				},
				"city": {
					"type": "string",
					"default": ""
				},
				"state": {
					"type": "string",
					"default": ""
				},
				"country": {
					"type": "string",
					"default": ""
				},
				"zip_code": {
					"type": "string",
					"default": ""
				}
			},
			"required": [
				"line1"
			]
		}
	},
	"required": [
		"first_name",
		"last_name",
		"date_of_birth"
	]
}